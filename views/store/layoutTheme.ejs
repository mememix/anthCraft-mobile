<div class="layout theme scroll">
		{{themes.forEach(function(theme) { }}
		<div class="layout-container ">
			<a href="/store/theme/{{=theme._id}}" class="" target="_blank">
				<div class="loader">
					<img class="thum" src="{{=THEME_THUMBNAIL_PATH}}{{=theme.thumbnail}}"></img>
					<img class="loading" src="/images/default.png"></img>
				</div>
				<p class="info"><span class="ellipsis">{{=theme.title}}</span><span class="pull-right price">Free</span></p>
			</a>
		</div>
		{{ }) }}
		<!-- <div class="clearfix more"> -->
			<a class="btn cy-btn" href="/store/more/theme?page=2">More</a>
		<!-- </div> -->
</div>
