<div class="layout wallpaper scroll">
	<div class="col-xs-offset-8 text-center">
		<label for="up-wallpaper" class="cy-file-btn">
			<i class="icon icon-folder"></i>
			File
		</label>
		<input id="up-wallpaper" type="file" style="display:none"/>
	</div>
	{{ wallpapers.forEach(function(wallpaper) { }}
		<div class="col-xs-6">
			<div class="layout-container">
				<div class="loader">
					<img class="thum" src="{{RESOURCE_PATH}}{{wallpaper.files[0].path}}"></img>
					<img class="loading" src="/images/default.png"></img>
				</div>
			</div>
		</div>
	{{ }) }}

	{{ if(wallpapers && wallpapers.length) { }}
	<div class="clearfix more">
		<a class="btn cy-btn" href="/design/more/wallpaper?page=1">More</a>
	</div>
	{{ } }}
</div>
