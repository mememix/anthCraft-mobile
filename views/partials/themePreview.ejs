<div class="layout theme-preview">
	<div id="slider" class="swipe">
		<div class="swipe-wrap">
			{{screens.forEach(function(screen) { }}
			<div class="preview-container">
				<img src="{{=screen.src}}"></img>
			</div>
			{{ }) }}
		</div>
	</div>

	<div class="menu-bar">
		{{screens.forEach(function(screen) { }}
		<a class="cy-dot-btn btn" role="button">
		</a>
		{{ }) }}
	</div>

</div>
